<body>
  <script>
      let num = parseInt(prompt("DIGITE UM NÃšMERO:"));
      function impares(inicio = 0, fim = 100) {
          
          if (inicio > fim) {
              [inicio, fim] = [fim, inicio];
          }

          let resultado = '';

          
          for (let i = inicio; i <= fim; i++) {
              if (i % 2 !== 0) {
                  resultado += i + ' ';
              }
          }

          return resultado.trim();
      }

      let resultado = imprimirImpares(num, 100);
      console.write(resultado);

  </script>

